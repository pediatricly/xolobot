server {
    server_name zoey.pediatricly.com;

    location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
	alias /home/mdscahill/xolo/staticfiles/;
        expires 30d;
        add_header Cache-Control "public";
    }

    location / {
        include proxy_params;
        proxy_pass http://unix:/home/mdscahill/xolo/xolo.sock;
    }

    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/zoey.pediatricly.com/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/zoey.pediatricly.com/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot

}
server {
    if ($host = zoey.pediatricly.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    listen 80;
    server_name zoey.pediatricly.com;
    return 404; # managed by Certbot


}